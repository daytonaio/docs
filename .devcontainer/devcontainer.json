{
    "name": "Daytona Docs",
    "build": {
      "dockerfile": "Dockerfile"
    },
    "remoteUser": "root",
    "postCreateCommand": "yarn install",
    "postStartCommand": "yarn start",
    "waitFor": "postCreateCommand",
    "customizations": {
      "codespaces": {
        "openFiles": ["CONTRIBUTING.md"]
      },
      "vscode": {
        "extensions": ["astro-build.astro-vscode",
         "esbenp.prettier-vscode"]
      }
    },
    "forwardPorts": [4321]
  }